{
  "name": "super-dom-x",
  "version": "1.0.0",
  "description": "SuperDom X ion",
  "author": "@xopherdeep <heylisten@xtiv.net>",
  "repository": {
    "type": "git",
    "url": "git@heroku.com:hero-superdomx-com"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build:js": "mkdir -p www.superdomx/dist/js && webpack -p --config webpack.config.build --colors --progress",
    "watch:js": "mkdir -p www.superdomx/site/js && webpack --watch --colors --progress",
    "build:css": "mkdir -p www.superdomx/dist/css && lessc --clean-css='--s0' --autoprefix='last 2 versions' www.superdomx/src/less/main.less www.superdomx/dist/css/app.css",
    "watch:css": "mkdir -p www.superdomx/site/css && watch 'lessc www.superdomx/src/less/main.less www.superdomx/site/css/app.css --autoprefix=\"last 2 versions\"' www.superdomx/src/less/ -du",
    "build:html": "mkdir -p www.superdomx/dist/ && jade www.superdomx/src/pages -o www.superdomx/dist/",
    "watch:html": "mkdir -p www.superdomx/site/ && jade -w www.superdomx/src/pages -o www.superdomx/site/",
    "clean:build": "rm -Rf www.superdomx/dist/",
    "clean:dev": "rm -Rf www.superdomx/site/",
    "clean": "npm run clean:build && npm run clean:dev",
    "build": "npm run clean:build && npm run build:html && npm run build:js && npm run build:css && grunt ",
    "specs": "node_modules/karma/bin/karma start",
    "serve": "nodemon server/index.js --ignore www.superdomx/src/ --ignore www.superdomx/site/ --ignore server/public/",
    "test": "npm run specs -- --single-run --reporters dots",
    "start": "npm run clean:dev && parallelshell 'npm run watch:js' 'npm run watch:html' 'npm run watch:css' 'npm run serve' 'grunt' 'grunt watch' ",
    "postinstall": "npm run build"
  },
  "babel": {
    "plugins": [
      "babel-plugin-mjsx"
    ]
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-plugin-mjsx": "^1.0.3",
    "browser-sync": "^2.7.3",
    "express": "^4.13.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compass": "^0.9.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-rename": "0.0.3",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-handlebars-layouts": "0.1.7",
    "grunt-replace": "0.11.0",
    "handlebars": "1.0.6",
    "jade": "^1.11.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-fixture": "^0.2.4",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-html-reporter": "^0.1.8",
    "karma-jasmine-matchers": "^2.0.0-beta1",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.5.1",
    "less": "^2.5.0",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-clean-css": "^1.5.0",
    "lodash": "0.9.2",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.8.0",
    "parallelshell": "^2.0.0",
    "phantomjs": ">=1.9",
    "phantomjs-polyfill": "0.0.1",
    "socket.io": "^1.3.6",
    "watch": "^0.16.0",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "jquery": "^1.9.1",
    "lodash": "^0.9.2",
    "mithril": "^0.2.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compass": "^0.9.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-rename": "0.0.3",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-handlebars-layouts": "^0.1.6"
  }
}
